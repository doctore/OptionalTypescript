{
  "name": "java-optional-typescript",
  "version": "1.0.0",
  "description": "Java Optional for Typescript",
  "main": "dist/optional.type.js",
  "files": [
    "dist"
  ],
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "start": "npm run build",
    "clean": "rimraf coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts' --format stylish",
    "check": "npm run lint -s && dependency-check ./package.json",
    "pretest": "tsc",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "compile": "tsc",
    "build": "npm run compile",
    "postbuild": "npm run check -s && npm run test -s",
    "coverage": "nyc npm run test",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "autocl": "auto-changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doctore/OptionalTypescript.git"
  },
  "keywords": [
    "Typescript",
    "Optional"
  ],
  "author": "doctore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doctore/OptionalTypescript/issues"
  },
  "homepage": "https://github.com/doctore/OptionalTypescript#readme",
  "dependencies": {
    "npm": "6.5.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "5.0.0",
    "@types/node": "9.6.5",
    "@types/rimraf": "2.0.2",
    "auto-changelog": "1.9.0",
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "dependency-check": "3.2.0",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "13.1.0",
    "prettier": "1.14.3",
    "rimraf": "2.6.3",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "check-coverage": true
  }
}